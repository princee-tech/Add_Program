class Compute 
{
    static pair getMinMax(long a[], long n)  
    {
        long min =a[0];
        long max=a[0];
        
       for(int i=0;i<=n-1;i++){
           if(a[i]>max){
           max=a[i];
           }
          
           if(a[i]<min){
               min=a[i];
           }
           
           
       }
       pair p=new pair(min,max);
       return  p;
    }
}
